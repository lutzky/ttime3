  "main": "main.js",
  "dependencies": {
    "@types/bootstrap": "^5.2.6",
    "@types/chai": "^4.3.4",
    "@types/jquery": "^3.5.16",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.7.23",
    "@types/selectize": "^0.12.35",
    "chai": "^4.3.7",
    "js-beautify": "^1.14.7",
    "karma": "^6.4.1",
